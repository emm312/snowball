
import System


class Parent {
    pub var hello: String = ""
    pub operator new() {}

    pub virt fn hello() {
        System.println("parent")
    }
}

class Child : Parent {
    // pub var hello: String = "hey"

    pub operator new() {}

    pub virt fn hello() {
        System.println(self.hello)
    }
}

pub fn main() Void {

    var h = new Child()
    h.hello()
}