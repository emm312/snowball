
import System

fn hello<T>() {
    System.print((new T()).hey())
}


class Hello {

    pub operator new() {

    }

    pub fn hey() String {
        return "sup"
    }

    pub virt fn hello() {
        System.println("hello")
    }
}

pub fn main() Void {


    hello!<Hello>()

    var h = new Hello()
    h.hello()
}